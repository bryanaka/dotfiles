alias aliases='vim ~/.dotfiles/aliases'

# Git
alias gp='git push'

# Google Chrome
alias chrome='/Applications/Google\ Chrome.app/Contents/MacOS/Google\ Chrome'
alias canary='/Applications/Google\ Chrome\ Canary.app/Contents/MacOS/Google\ Chrome\ Canary'

# Projects
alias s-py='cd ~/projects/consulting/shippo/shippo && vssh'
alias s-js='cd ~/projects/consulting/shippo/shippo-frontend && ems'

# Ember
alias ems='ember server'
alias emt='ember test'
alias emg='ember generate'

# Yarn
alias yt='yarn test'
alias yi='yarn install'
alias yr='yarn run'

# Vagrant
alias vssh='vagrant ssh -- -A'
